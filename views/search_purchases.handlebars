<div class="CSSTableGenerator" >
<table class="col-xs-8">
	<tr>
		<thead>
			<td>Id</td>
			<td>Product Name</td>
			<td>Date</td>
			<td>Price</td>
			<td>Supplier Name</td>
		</thead>
		
	</tr>

	<tbody>
		{{#purchases}}

		<tr>
			<td>{{id}}</td>
			<td>{{product_name}}</td>
			<td>{{stock_date}}</td>
			<td>{{cost_price}}</td>
			<td>{{supplier_name}}</td>
		</tr>
		{{/purchases}}
	</tbody>
	<br>
	<br>
	<a href="/purchases"><button>Show All</button></a>
</table>